<form version="1.1" script="SA-Investigator:tabs.js" stylesheet="SA-Investigator:tabs.css" theme="dark">
  <label>Notable and Analyst Review (NAR)</label>
  <search id="baseSearch">
    <earliest>$notable_time.earliest$</earliest>
    <latest>$notable_time.latest$</latest>
    <query>
    	`notable`
    	| search NOT (eventtype=notable_suppression-* OR suppression=*)
    	| fields *
    	| eval date_hour=strftime(_time, "%H") 
      | eval date_day=strftime(_time, "%a")
      | eval onglass=case((date_day IN("Mon","Tue","Wed","Thu","Fri") AND date_hour&gt;=6 AND date_hour&lt;=18),"true",true(),"false")
	    </query>
  </search>
  <fieldset submitButton="true" autoRun="true">
    <input type="multiselect" token="notable_owner">
      <label>Owner</label>
      <delimiter> ,</delimiter>
      <fieldForLabel>realname</fieldForLabel>
      <fieldForValue>realname</fieldForValue>
      <search>
        <query>| inputlookup notable_owners_lookup</query>
      </search>
      <choice value="*">All</choice>
      <default>*</default>
      <prefix>owner_realname IN (</prefix>
      <suffix>)</suffix>
      <valuePrefix>"</valuePrefix>
      <valueSuffix>"</valueSuffix>
    </input>
    <input type="multiselect" token="notable_status">
      <label>Status</label>
      <delimiter> ,</delimiter>
      <fieldForLabel>notable_status</fieldForLabel>
      <fieldForValue>label</fieldForValue>
      <search>
        <query>| inputlookup reviewstatuses_lookup | dedup label
| where label!="Test Content"</query>
      </search>
      <choice value="*">All</choice>
      <default>*</default>
      <prefix>status_label IN (</prefix>
      <suffix>)</suffix>
      <valuePrefix>"</valuePrefix>
      <valueSuffix>"</valueSuffix>
    </input>
    <input type="multiselect" token="notable_dispos">
      <label>Disposition</label>
      <fieldForLabel>label</fieldForLabel>
      <fieldForValue>label</fieldForValue>
      <search>
        <query>| inputlookup disposition_lookup | dedup label</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <delimiter> ,</delimiter>
      <choice value="*">All</choice>
      <default>*</default>
      <prefix>disposition_label IN (</prefix>
      <suffix>)</suffix>
      <valuePrefix>"</valuePrefix>
      <valueSuffix>"</valueSuffix>
    </input>
    <input type="multiselect" token="notable_name">
      <label>Notable</label>
      <delimiter> ,</delimiter>
      <fieldForLabel>rule_name</fieldForLabel>
      <fieldForValue>rule_name</fieldForValue>
      <search>
        <query>| from inputlookup:incident_review_lookup | dedup rule_name | table rule_name | sort rule_name</query>
      </search>
      <choice value="*">All</choice>
      <default>*</default>
      <prefix>rule_name IN (</prefix>
      <suffix>)</suffix>
      <valuePrefix>"</valuePrefix>
      <valueSuffix>"</valueSuffix>
    </input>
    <input type="dropdown" token="content_type">
      <label>Content Type</label>
      <choice value="status_label!=&quot;Test Content&quot;">Production</choice>
      <choice value="status_label=&quot;Test Content&quot;">Test</choice>
      <choice value="*">All</choice>
      <default>status_label!="Test Content"</default>
    </input>
    <input type="dropdown" token="onglass_toggle">
      <label>On Glass Time</label>
      <choice value="true">Yes</choice>
      <choice value="false">No</choice>
      <choice value="*">All</choice>
      <default>*</default>
    </input>
    <input type="time" token="notable_time">
      <label>Time</label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <html>
        <br/>
      </html>
    <input type="text" token="user_dm">
      <label>User</label>
      <default></default>
      <change>
        <unset token="form.user_ident"></unset>
      </change>
    </input>
    <input type="dropdown" token="user_ident" searchWhenChanged="true">
      <label>Correlated Identities</label>
      <search>
        <query>| `reverse_identity_lookup("$user_dm$")` | append [ | makeresults | eval key="$user_dm$" | eval seed="$user_dm$" ] | dedup key | rex field=identity mode=sed "s/\\\/\\\\\\\/" | eval user_ident=if(isnull(identity),seed,mvjoin(identity, ",")) | table user_ident</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <selectFirstChoice>true</selectFirstChoice>
      <fieldForLabel>user_ident</fieldForLabel>
      <fieldForValue>user_ident</fieldForValue>
      <change>
        <condition match="len($value$) &gt; 0">
          <set token="user_ident" submit="false">(user IN ($value$))</set>
        </condition>
        <condition match="len($value$) == 0">
          <set token="user_ident" submit="false"></set>
        </condition>
      </change>
    </input>
    <input type="text" token="notable_src_form">
      <label>Source</label>
      <default></default>
      <change>
        <condition match="len($value$) &gt; 0">
          <set token="notable_src" submit="false">src=$value$</set>
        </condition>
        <condition match="len($value$) == 0">
          <set token="notable_src" submit="false"></set>
        </condition>
      </change>
    </input>
    <input type="text" token="notable_dest_form">
      <label>Destination</label>
      <default></default>
      <change>
        <condition match="len($value$) &gt; 0">
          <set token="notable_dest" submit="false">dest=$value$</set>
        </condition>
        <condition match="len($value$) == 0">
          <set token="notable_dest" submit="false"></set>
        </condition>
      </change>
    </input>
    <input type="text" token="notable_kw_form">
      <label>Keyword</label>
      <default></default>
      <change>
        <condition match="len($value$) &gt; 0">
          <set token="notable_kw" submit="false">(comment IN ("*$value$*") OR src IN ("*$value$*") OR dest IN ("*$value$*") OR user IN ("*$value$*"))</set>
        </condition>
        <condition match="len($value$) == 0">
          <set token="notable_kw" submit="false"></set>
        </condition>
      </change>
    </input>
  </fieldset>
  <row id="tabs">
    <panel>
      <html>
				<ul id="tabs" class="nav nav-tabs">
                    <li class="active">
                        <a href="#" class="toggle-tab" data-toggle="tab" data-elements="tab_overview_1,tab_overview_2,tab_overview_3" data-token="control_overview">Overview</a>
                    </li>
					          <li>
						            <a href="#" class="toggle-tab" data-toggle="tab" data-elements="tab_a_detail_1,tab_a_detail_2,tab_a_detail_3" data-token="control_analyst_detail">Analyst Details</a>
					          </li>
					          <li>
						            <a href="#" class="toggle-tab" data-toggle="tab" data-elements="tab_n_detail_1,tab_n_detail_2,tab_n_detail_3,tab_n_detail_4" data-token="control_notable_detail">Notable Details</a>
					          </li>					          
				</ul>
			</html>
    </panel>
  </row>
  <row id="tab_overview_1">
    <panel>
      <chart id="notables_overtime_1">
        <title>Notables - Over Time</title>
        <search base="baseSearch">
          <query>| search $notable_owner$ $notable_status$ $notable_dispos$ $notable_name$ $user_ident$ $notable_src$ $notable_dest$ $notable_kw$ onglass=$onglass_toggle|s$
| search $content_type$
| timechart count usenull=false limit=20 span=1d
| eventstats avg("count") as "average"</query>
        </search>
